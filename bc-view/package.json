{
  "name": "beancounter-app",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "start": "razzle start --inspect=51089",
    "start:demo": "SVC_POSITION=http://localhost:9600 SVC_DATA=http://localhost:9610 KAFKA_TOPIC_TRN=bc-trn-csv-demo razzle start --inspect=51089",
    "build": "razzle build --noninteractive",
    "start:prod": "NODE_ENV=production node build/server.js",
    "prettier": "prettier --write 'src/**/*.{ts*,json,md,yml,js}' && yarn lint",
    "prettier:check": "prettier  --check 'src/**/*.ts*' && yarn lint",
    "lint": "eslint 'src/**/*.ts*' && yarn lint-md",
    "lint-md": "remark .",
    "test": "razzle test --env=jest-environment-jsdom-sixteen",
    "package": "yarn build --prod && npx pkg . -t node14-alpine-x64 --output app"
  },
  "dependencies": {
    "@creativebulma/bulma-tooltip": "^1.0.2",
    "@fortawesome/fontawesome-free": "^5",
    "@hookform/resolvers": "^2.0.0-rc.1",
    "@react-keycloak/core": "^3.2.0",
    "@react-keycloak/ssr": "^3.3.0",
    "axios": "^0.21.1",
    "bulma": "^0.9.0",
    "bulma-pageloader": "^0.3.0",
    "bulma-spacing": "^2.1.5",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "i18next": "^19.0.2",
    "i18next-browser-languagedetector": "^6.0.1",
    "i18next-express-middleware": "^1.8.2",
    "i18next-http-backend": "^1.2.1",
    "i18next-node-fs-backend": "^2.1.3",
    "kafka-node": "^5",
    "keycloak-js": "^12.0.3",
    "moment": "^2.29",
    "papaparse": "^5.3.0",
    "prop-types": "^15.7.2",
    "ramda": "^0.27.0",
    "razzle": "^4",
    "react": "^17",
    "react-dom": "^17",
    "react-dropzone": "^11",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^6",
    "react-i18next": "^11",
    "react-loadable": "^5.5.0",
    "react-number-format": "^4.5",
    "react-router": "^5.2.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.1.2",
    "react-select": "^4.1.0",
    "react-switch": "^5.0.1",
    "winston": "^3",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/helper-call-delegate": "^7.12.13",
    "@babel/preset-env": "^7.12.16",
    "@emotion/jest": "^11.2.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/axios": "^0.14.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.16.1",
    "@types/i18next-node-fs-backend": "^2.1.0",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.14.10",
    "@types/ramda": "^0.27.17",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^16.9.2",
    "@types/react-helmet": "^6.1.0",
    "@types/react-loadable": "^5.5.2",
    "@types/react-router": "^5.1.2",
    "@types/react-router-config": "^5.0",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-select": "^4.0.13",
    "@types/store": "^2.0.2",
    "@types/webpack": "^5",
    "@types/webpack-env": "^1.1",
    "@typescript-eslint/eslint-plugin": "^4",
    "@typescript-eslint/parser": "^4",
    "autoprefixer": "^10",
    "babel-jest": "^26.3.0",
    "babel-preset-razzle": "^4.0.3",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^24",
    "eslint-plugin-jest-dom": "^3.6.5",
    "eslint-plugin-prettier": "^3",
    "eslint-plugin-react": "^7",
    "eslint-plugin-react-hooks": "latest",
    "eslint-plugin-testing-library": "latest",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^4",
    "jest": "^26.6",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jest-junit": "^12",
    "js-levenshtein": "^1.1.6",
    "lint-staged": "^10",
    "mini-css-extract-plugin": "0.9.0",
    "nock": "^13",
    "postcss": "8.2.4",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "pretty-format": "^26",
    "razzle-dev-utils": "^4.0.3",
    "razzle-plugin-scss": "^4.0.3",
    "react-hot-loader": "^4.13.0",
    "remark-cli": "^9.0",
    "remark-lint": "^8.0",
    "remark-lint-no-literal-urls": "^2.0.0",
    "remark-preset-lint-recommended": "^5.0.0",
    "style-loader": "^2.0.0",
    "ts-jest": "^26",
    "typescript": "^4.2",
    "webpack": "^5",
    "webpack-dev-server": "^3.11"
  },
  "bin": "build/server.js",
  "pkg": {
    "scripts": "build/public/static2/**/*.js",
    "assets": [
      "build/public/static2/**/*.svg",
      "build/public/static2/**/*.css",
      "node_modules/**/*.sass"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx,md}": [
      "prettier",
      "git add"
    ]
  },
  "jest-junit": {
    "output": "reports/junit/junit.xml",
    "usePathForSuiteName": "true"
  },
  "jest": {
    "coverageReporters": [
      "text"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest",
      "\\.css$": "<rootDir>/node_modules/razzle/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/node_modules/razzle/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [],
    "testMatch": [
      "<rootDir>/src/**/**/__tests__/**/*.(ts|js)?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(ts|js)?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ]
  }
}
